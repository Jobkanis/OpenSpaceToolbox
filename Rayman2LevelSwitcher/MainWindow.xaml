                <TreeViewItem x:Name="learn_60" Header="Fairy Glade 5" HorizontalAlignment="Left" Width="213"/>
            </TreeViewItem>
            <TreeViewItem Header="The Marshes of Awakening">
                <TreeViewItem x:Name="ski_10" Header="Marshes 1" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="ski_60" Header="Marshes 2" HorizontalAlignment="Left" Width="213"/>
            </TreeViewItem>
            <TreeViewItem Header="The Bayou">
                <TreeViewItem x:Name="chase_10" Header="Bayou 1" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="chase_22" Header="Bayou 2" HorizontalAlignment="Left" Width="213"/>
            </TreeViewItem>
            <TreeViewItem Header="The Sanctuary of Water and Ice">
                <TreeViewItem x:Name="water_10" Header="Sanctuary of Water and Ice 1" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="water_20" Header="Sanctuary of Water and Ice 2" HorizontalAlignment="Left" Width="213"/>
            </TreeViewItem>
            <TreeViewItem Header="The Menhir Hills">
                <TreeViewItem x:Name="rodeo_10" Header="Menhir Hills 1" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="rodeo_40" Header="Menhir Hills 2" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="rodeo_60" Header="Menhir Hills 3" HorizontalAlignment="Left" Width="213"/>
            </TreeViewItem>
            <TreeViewItem Header="The Cave of Bad Dreams">
                <TreeViewItem x:Name="vulca_10" Header="Cave of Bad Dreams 1" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="vulca_20" Header="Cave of Bad Dreams 2" HorizontalAlignment="Left" Width="213"/>
            </TreeViewItem>
            <TreeViewItem Header="The Canopy">
                <TreeViewItem x:Name="glob_30" Header="Canopy 1" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="glob_10" Header="Canopy 2" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="glob_20" Header="Canopy 3" HorizontalAlignment="Left" Width="213"/>
            </TreeViewItem>
            <TreeViewItem Header="Whale Bay">
                <TreeViewItem x:Name="whale_00" Header="Whale Bay 1" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="whale_05" Header="Whale Bay 2" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="whale_10" Header="Whale Bay 3" HorizontalAlignment="Left" Width="213"/>
            </TreeViewItem>
            <TreeViewItem Header="The Sanctuary of Stone and Fire">
                <TreeViewItem x:Name="plum_00" Header="Stone and Fire 1" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="plum_20" Header="Stone and Fire 1-B" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="plum_10" Header="Stone and Fire 2" HorizontalAlignment="Left" Width="213"/>
            </TreeViewItem>
            <TreeViewItem Header="The Echoing Caves">
                <TreeViewItem x:Name="bast_10" Header="Echoing Caves 1" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="cask_10" Header="Echoing Caves 2" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="cask_30" Header="Echoing Caves 3" HorizontalAlignment="Left" Width="213"/>
            </TreeViewItem>
            <TreeViewItem Header="The Precipice">
                <TreeViewItem x:Name="nave_10" Header="Precipice 1" HorizontalAlignment="Left" Width="213"/>
