                <TreeViewItem x:Name="menu" Header="Menu" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="mapmonde" Header="The Hall of Doors" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="ly_10" Header="Walk of Life" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="ly_20" Header="Walk of Power" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="bonux" Header="Bonus Level" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="raycap" Header="Score Screen" HorizontalAlignment="Left" Width="213"/>
            </TreeViewItem>
            <TreeViewItem Header="Cutscenes">
                <TreeViewItem x:Name="nego_10" Header="Council Chamber of the Teensies Cutscene" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="batam_10" Header="Meanwhile... (Razorbeard Cutscenes)" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="bast_09" Header="Echoing Caves Cutscene" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="ball" Header="Balloon Cutscene (Iron Mountains)" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="batam_20" Header="Prison Ship Razorbeard Cutscene" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="end_10" Header="The Great Escape" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="staff_10" Header="Credits" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="poloc_10" Header="Polokus Mask 1" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="poloc_20" Header="Polokus Mask 2" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="poloc_30" Header="Polokus Mask 3" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="poloc_40" Header="Polokus Mask 4" HorizontalAlignment="Left" Width="213"/>
            </TreeViewItem>
            <TreeViewItem Header="Intro/Woods of Light">
                <TreeViewItem x:Name="jail_10" Header="Intro" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="jail_20" Header="Jail" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="learn_10" Header="Woods of Light" HorizontalAlignment="Left" Width="213"/>
            </TreeViewItem>
            <TreeViewItem Header="Fairy Glade">
                <TreeViewItem x:Name="learn_30" Header="Fairy Glade 1" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="learn_31" Header="Fairy Glade 2" HorizontalAlignment="Left" Width="213"/>
