                <TreeViewItem x:Name="nave_20" Header="Precipice 3" HorizontalAlignment="Left" Width="213"/>
            </TreeViewItem>
            <TreeViewItem Header="The Top of the World">
                <TreeViewItem x:Name="seat_10" Header="Top of the World 1" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="seat_11" Header="Top of the World 2" HorizontalAlignment="Left" Width="213"/>
            </TreeViewItem>
            <TreeViewItem Header="The Sanctuary of Rock and Lava">
                <TreeViewItem x:Name="earth_10" Header="Sanctuary of Rock and Lava 1" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="earth_20" Header="Sanctuary of Rock and Lava 2" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="earth_30" Header="Sanctuary of Rock and Lava 3" HorizontalAlignment="Left" Width="213"/>
            </TreeViewItem>
            <TreeViewItem Header="Beneath the Sanctuary of Rock and Lava">
                <TreeViewItem x:Name="helic_10" Header="Beneath Rock and Lava 1" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="helic_20" Header="Beneath Rock and Lava 2" HorizontalAlignment="Left" Width="213"/>
                <TreeViewItem x:Name="helic_30" Header="Beneath Rock and Lava 3" HorizontalAlignment="Left" Width="213"/>
            </TreeViewItem>
            <TreeViewItem Header="Tomb of the Ancients">
                <TreeViewItem x:Name="morb_00" Header="Tomb 1" HorizontalAlignment="Left" Width="213"/>
