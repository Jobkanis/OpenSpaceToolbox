<Window x:Class="Rayman2LevelSwitcher.RenameDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Rayman2LevelSwitcher"
        mc:Ignorable="d"
        Title="Rename Bookmark" Height="97.25" Width="291.75" WindowStyle="None" ResizeMode="NoResize" WindowStartupLocation="Manual" Loaded="Window_Loaded">
    <Border BorderBrush="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}" BorderThickness="1">
        <Grid>
            <Label Content="Rename bookmark:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
            <TextBox x:Name="txtbox_name" Height="23" Margin="10,41,10,0" TextWrapping="Wrap" VerticalAlignment="Top" KeyDown="txtbox_name_KeyDown"/>
            <Button x:Name="btn_rename" Content="Rename" HorizontalAlignment="Left" Margin="10,69,0,0" VerticalAlignment="Top" Width="75" KeyboardNavigation.IsTabStop="False" Click="btn_rename_Click"/>
            <Button x:Name="btn_close" Content="Cancel" HorizontalAlignment="Left" Margin="205,69,0,0" VerticalAlignment="Top" Width="75" Click="btn_close_Click" KeyboardNavigation.IsTabStop="False"/>
        </Grid>
    </Border>
</Window>
